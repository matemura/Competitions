#include <cstdio>

using namespace std;

int n;
int d[128] = { 1, 2, 3, 5, 7, 11, 15, 22, 30, 41, 55, 75, 99, 131, 169, 219,
               280, 355, 444, 556, 695, 860, 1054, 1294, 1581, 1910, 2300,
               2768, 3311, 3946, 4693, 5555, 6566, 7730, 9061, 10608, 12383,
               14425, 16770, 19468 };

int main()
{
    scanf("%d", &n);
    printf("%d\n", d[n - 1]);
    return 0;
}
