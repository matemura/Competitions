
#include <cstdio>
using namespace std;

char str[] = ",1,2,3,5,7,11,15,22,30,41,55,75,97,129,166,213,272,343,430,536,664,822,1008,1230,1495,1808,2178,2616,3122,3720,4416,5221,6164,7249,8497,9941,11593,13481,15665,18150,20971,24184,27827,31974,36650,41944,47930,54670,62256,70786,80369,91112,103154,116602,131678,148498,167233,188134,211415,237274,266013,297967,333355,372567,416029,464134,517244,575923,640708,712174,790933,877625,973112,1078082,1193425,1320094,1459178,1611738,1778922,1962030,";

int main() {
    int N;
    scanf("%d", &N);
    for (int i = 0; true; ++i)
        if (str[i] == ',') {
            --N;
            if (N == 0) {
                for (int j = i + 1; str[j] != ','; ++j)
                    putchar(str[j]);
                puts("");
                return 0;
            }
        }
    return 0;
}
